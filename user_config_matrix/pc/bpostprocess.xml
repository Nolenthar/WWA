<?xml version="1.0" encoding="UTF-16"?>
<UserConfig>
	<Group id="PostProcessUGOM" displayName="Mods.Ultimate Graphics Options.postprocess" tags="graphics">
		<PresetsArray>
			<Preset id="0" displayName="low">
				<Entry varId="AllowBloom" 		 value="true"/>
				<Entry varId="AllowShafts" 	 	 value="false"/>
				<Entry varId="AllowAntialias" 	 value="false"/>
				<Entry varId="AllowBlur" 		 value="false"/>
				<Entry varId="AllowDOF" 		 value="false"/>
				<Entry varId="AllowVignette" 	 value="true"/>
				<Entry varId="Virtual_SharpenAmount" value="0"/>
				<Entry varId="Virtual_SSAOSolution" value="0"/>
				<Entry varId="AllowMotionBlur"   value="false"/>
				<Entry varId="AllowChromaticAberration"  value="false"/>
			</Preset>
			<Preset id="1" displayName="medium">
				<Entry varId="AllowBloom" 		 value="true"/>
				<Entry varId="AllowShafts" 	 	 value="false"/>
				<Entry varId="AllowAntialias" 	 value="true"/>
				<Entry varId="AllowBlur" 		 value="true"/>
				<Entry varId="AllowDOF" 		 value="false"/>
				<Entry varId="AllowVignette" 	 value="true"/>
				<Entry varId="Virtual_SharpenAmount" value="1"/>
				<Entry varId="Virtual_SSAOSolution" value="1"/>
				<Entry varId="AllowMotionBlur"   value="true"/>
				<Entry varId="AllowChromaticAberration"  value="false"/>
			</Preset>
			<Preset id="2" displayName="high">
				<Entry varId="AllowBloom" 		 value="true"/>
				<Entry varId="AllowShafts" 	 	 value="true"/>
				<Entry varId="AllowAntialias" 	 value="true"/>
				<Entry varId="AllowBlur" 		 value="true"/>
				<Entry varId="AllowDOF" 		 value="true"/>
				<Entry varId="AllowVignette" 	 value="true"/>
				<Entry varId="Virtual_SharpenAmount" value="2"/>
				<Entry varId="Virtual_SSAOSolution" value="1"/>
				<Entry varId="AllowMotionBlur"   value="true"/>
				<Entry varId="AllowChromaticAberration"  value="true"/>
			</Preset>
		</PresetsArray>
		<VisibleVars>
            <Var overrideGroup="PostProcess"	id="AllowMotionBlur" 	 displayName="allow_motion_blur" 	displayType="TOGGLE"/>
			<Var id="MotionBlurPositionTreshold" 	 displayName="Motion Blur Position Treshold" 	displayType="OPTIONS" tags="refreshEngine;customNames;nonLocalized;customDisplayName">
				<OptionsArray>
					<Option id="0" displayName="0.01">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="0.01"/>
					</Option>
					<Option id="1" displayName="0.03">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="0.03"/>
					</Option>
					<Option id="2" displayName="0.05">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="0.05"/>
					</Option>
					<Option id="3" displayName="0.1">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="0.1"/>
					</Option>
					<Option id="4" displayName="1">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="1.0"/>
					</Option>
				</OptionsArray>
			</Var>
			<Var id="MotionBlurRotationTreshold" 	 displayName="Motion Blur Rotation Treshold" 	displayType="OPTIONS" tags="refreshEngine;customNames;nonLocalized;customDisplayName">
				<OptionsArray>
					<Option id="0" displayName="0.5">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="0.5"/>
					</Option>
					<Option id="1" displayName="1">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="1.0"/>
					</Option>
					<Option id="2" displayName="2">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="2.0"/>
					</Option>
					<Option id="3" displayName="3">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="3.0"/>
					</Option>
					<Option id="4" displayName="4">
						<Entry overrideGroup="PostProcess"	varId="MotionBlurPositionTreshold" value="4.0"/>
					</Option>
				</OptionsArray>
			</Var>
            <Var overrideGroup="PostProcess"	id="AllowBlur" 		 displayName="allow_blur" 			displayType="TOGGLE"/>
            <Var overrideGroup="PostProcess"	id="AllowAntialias" 	 displayName="allow_Antialias" 		displayType="TOGGLE"/>
			<Var overrideGroup="PostProcess"	id="EnableTemporalAA"   displayName="Enable Temporal AA			<u>less texture flickering<u>" displayType="TOGGLE" tags="refreshEngine;customNames;nonLocalized;customDisplayName"/>
			<Var overrideGroup="PostProcess"	id="AllowMSAA"   displayName="Allow MSAA				<u>not working?<u>" displayType="TOGGLE" tags="refreshEngine;customNames;nonLocalized;customDisplayName"/>
			<Var id="MsaaLevel" 	 displayName="Msaa Level					<u>not working?<u>" 	displayType="OPTIONS" tags="refreshEngine;customNames;nonLocalized;customDisplayName">
				<OptionsArray>
					<Option id="0" displayName="0">
						<Entry overrideGroup="Rendering"	varId="MsaaLevel" value="0"/>
					</Option>
					<Option id="1" displayName="1">
						<Entry overrideGroup="Rendering"	varId="MsaaLevel" value="1"/>
					</Option>
				</OptionsArray>
			</Var>
			<Var overrideGroup="PostProcess"	id="AllowBloom" 		 displayName="allow_bloom" 			displayType="TOGGLE"/>
            <Var id="Virtual_SharpenAmount" 	 displayName="allow_sharpen" 	displayType="OPTIONS" tags="customNames">
				<OptionsArray>
					<Option id="0" displayName="panel_mainmenu_option_value_off">
						<Entry overrideGroup="PostProcess"	varId="SharpenAmount" value="0"/>
					</Option>
					<Option id="1" displayName="preset_value_low">
						<Entry overrideGroup="PostProcess"	varId="SharpenAmount" value="1"/>
					</Option>
					<Option id="2" displayName="preset_value_high">
						<Entry overrideGroup="PostProcess"	varId="SharpenAmount" value="2"/>
					</Option>
				</OptionsArray>
			</Var>
            <Var id="Virtual_SSAOSolution" 		 displayName="ssao_solution" 		displayType="OPTIONS">
				<OptionsArray>
					<Option id="0" displayName="AONone">
						<Entry overrideGroup="PostProcess"	varId="SSAOSolution" value="0"/>
					</Option>
					<Option id="1" displayName="SSAO">
						<Entry overrideGroup="PostProcess"	varId="SSAOSolution" value="1"/>
					</Option>
					<Option id="2" displayName="HBAO">
						<Entry overrideGroup="PostProcess"	varId="SSAOSolution" value="2"/>
					</Option>
				</OptionsArray>
			</Var>
            <Var overrideGroup="PostProcess"	id="AllowDOF" 		 	 displayName="allow_dof" 			displayType="TOGGLE"/>
            <Var overrideGroup="PostProcess"	id="AllowChromaticAberration" 	 displayName="allow_chromatic_aberration" 		displayType="TOGGLE"/>
			<Var overrideGroup="PostProcess"	id="AllowVignette" 	 displayName="allow_vignette" 		displayType="TOGGLE"/>
			<Var overrideGroup="PostProcess"	id="AllowShafts" 		 displayName="allow_shafts" 		displayType="TOGGLE"/>
		</VisibleVars>
	</Group>
</UserConfig>
